    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.1.0",
    "express": "^4.15.4",
    "localtunnel": "^1.8.3",
